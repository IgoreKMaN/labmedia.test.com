@media (max-width: 1920px) and (min-width: 945px)
	header
		border-bottom 3px solid #f2f2f2
		:after
			content '' 
			clear both
			display block 
		.header-wrapper
			margin 30px 0
			.menu
				padding-top 2%
				ul
					li
						display inline-block
						border-bottom 1px solid #999999
						font-weight bold
						margin-right 6%
						a
							
							color #000000
			.profil
				text-align right